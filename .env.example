# Logging
LOG_LEVEL=info
LOG_PRETTY=true

# Apollo Landing Page
ENABLE_APOLLO_LANDING_PAGE=false

# Database configuration
DB_USERNAME=postgres
DB_PASS=your_db_password
DB_HOST=localhost
DB_PORT=5422
DB_DATABASE=local

# RabbitMQ Event Bus
EVENT_BUS_RABBIT_MQ_URL=amqp://rabbitmq:password@localhost:5672/

# Redis Cache
CACHE_HOST=localhost
CACHE_PORT=6378

# Session configuration
SESSION_SECRET=your_session_secret
SESSION_SECURE=false
SESSION_SAME_SITE=lax

# CORS
CORS_ENABLED=true
CORS_ORIGIN=http://localhost:2211,*

# AI / HuggingFace
AI_HF_API_KEY=your_hf_api_key
AI_HF_MODEL_NAME=tgi
AI_HF_MODEL_URL=https://your-hf-endpoint/v1/
OPENAI_API_KEY=your_openai_api_key
ANTHROPIC_API_KEY=your_anthropic_api_key

# PostHog Analytics
POSTHOG_API_KEY=your_posthog_api_key
POSTHOG_HOST=https://eu.i.posthog.com

# Core API (GraphQL endpoint)
NEXT_PUBLIC_CORE_API_HOST_GRAPHQL=http://localhost:3000/graphql

# Customer UI
NEXT_PUBLIC_API_URL=http://localhost:3000
