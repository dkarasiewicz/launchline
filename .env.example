# Logging
LOG_LEVEL=info
LOG_PRETTY=true

# Apollo Landing Page
ENABLE_APOLLO_LANDING_PAGE=false

# Database configuration
DB_USERNAME=postgres
DB_PASS=your_db_password
DB_HOST=localhost
DB_PORT=5422
DB_DATABASE=local

# RabbitMQ Event Bus
EVENT_BUS_RABBIT_MQ_URL=amqp://rabbitmq:password@localhost:5672/

# Redis Cache
CACHE_HOST=localhost
CACHE_PORT=6378

# Session configuration
SESSION_SECRET=your_session_secret
SESSION_SECURE=false
SESSION_SAME_SITE=lax

# App URLs
APP_URL=http://localhost:3000
FRONTEND_URL=http://localhost:2211

# Integration encryption
INTEGRATION_ENCRYPTION_KEY=your_64_char_hex_key

# CORS
CORS_ENABLED=true
CORS_ORIGIN=http://localhost:2211,*

# AI / HuggingFace
PRIMARY_MODEL_API_KEY=your_primary_model_api_key
PRIMARY_MODEL_NAME=tgi
PRIMARY_MODEL_URL=https://your-primary-model-endpoint/v1/
PRIMARY_MODEL_MAX_RETRIES=3
FAST_MODEL_API_KEY=your_fast_model_api_key
FAST_MODEL_NAME=tgi
FAST_MODEL_URL=https://your-fast-model-endpoint/v1/
FAST_MODEL_MAX_RETRIES=3
REASONING_MODEL_API_KEY=your_reasoning_model_api_key
REASONING_MODEL_NAME=tgi
REASONING_MODEL_URL=https://your-reasoning-model-endpoint/v1/
REASONING_MODEL_MAX_RETRIES=3
ANALYSIS_MODEL_API_KEY=your_analysis_model_api_key
ANALYSIS_MODEL_NAME=tgi
ANALYSIS_MODEL_URL=https://your-analysis-model-endpoint/v1/
ANALYSIS_MODEL_MAX_RETRIES=3
MEMORY_EMBEDDING_TYPE=text-embedding-3-small
MEMORY_EMBEDDING_DIMS=1536
MEMORY_DEFAULT_SEARCH_LIMIT=20
MEMORY_MAX_IMPORTANCE=1.0
MEMORY_MIN_IMPORTANCE=0.0

# PostHog Analytics
POSTHOG_API_KEY=your_posthog_api_key
POSTHOG_HOST=https://eu.i.posthog.com
NEXT_PUBLIC_POSTHOG_KEY=your_posthog_api_key
NEXT_PUBLIC_POSTHOG_HOST=https://eu.i.posthog.com

#Langsmith
LANGSMITH_TRACING=true
LANGSMITH_ENDPOINT=https://eu.api.smith.langchain.com
LANGSMITH_API_KEY=your_langsmith_api_key
LANGSMITH_PROJECT="Launchline"

#Customer-ui
NEXT_PUBLIC_API_URL=http://localhost:2211
NEXT_PUBLIC_CORE_API_URL=http://localhost:3000

# Linear Integration
LINEAR_CLIENT_ID=
LINEAR_CLIENT_SECRET=
LINEAR_WEBHOOK_SECRET=

# Slack Integration
SLACK_CLIENT_ID=
SLACK_CLIENT_SECRET=
SLACK_SIGNING_SECRET=

# Google Integration
GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=

# GitHub Integration
GITHUB_CLIENT_ID=your_github_client_id
GITHUB_CLIENT_SECRET=your_github_client_secret
GITHUB_WEBHOOK_SECRET=your_github_webhook_secret
# GitHub App (preferred)
GITHUB_APP_ID=your_github_app_id
GITHUB_APP_SLUG=your_github_app_slug
GITHUB_APP_PRIVATE_KEY=your_github_app_private_key
